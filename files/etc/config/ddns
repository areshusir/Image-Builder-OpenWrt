
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'
	option services_url 'root'

config service 'HomeV4'
	option service_name 'cloudflare.com-v4'
	option use_ipv6 '0'
	option enabled '1'
	option lookup_host 'husir.yige.ru'
	option domain 'husir@yige.ru'
	option username 'areshusir@gmail.com'
	option password '23442fb3e8ed331d3afd7132e914fd20f6a31'
	option ip_source 'network'
	option ip_network 'wan'
	option interface 'wan'
	option use_syslog '2'
	option check_interval '8'
	option check_unit 'hours'
	option force_interval '7'
	option force_unit 'days'

config service 'HomeV6'
	option service_name 'cloudflare.com-v4'
	option use_ipv6 '1'
	option enabled '1'
	option lookup_host 'husir.yige.ru'
	option domain 'husir@yige.ru'
	option username 'areshusir@gmail.com'
	option password '23442fb3e8ed331d3afd7132e914fd20f6a31'
	option ip_source 'network'
	option ip_network 'lan'
	option interface 'lan'
	option use_syslog '2'
	option check_interval '8'
	option check_unit 'hours'
	option force_interval '7'
	option force_unit 'days'

